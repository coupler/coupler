<% if @results.empty? %>
<p>There are currently no results for this scenario.</p>
<% else %>
<table class="list">
  <thead>
    <tr>
      <th>#</th>
      <th>Run at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @results.each do |result| %>
    <tr>
      <td><%= result.run_number %></td>
      <td><%= result.created_at %></td>
      <td><a href="/projects/<%=@project.id%>/scenarios/<%=@scenario.id%>/results/<%=result.id%>">Show</a></td>
    </tr>
  <% end %>
  </tbody>
</table>
<% end %>
